{
  "type": "layout_builder",
  "id": "screen",
  "args": {
    "child": {
      "type": "scaffold",
      "args": {
        "appBar": {
          "type": "app_bar",
          "args": {
            "title": {
              "type": "text",
              "args": {
                "text": "LayoutBuilder"
              }
            }
          }
        },
        "body": {
          "type": "layout_builder",
          "id": "body",
          "args": {
            "child": {
              "type": "list_view",
              "args": {
                "children": [
                  {
                    "type": "container",
                    "id": "scroll_pre_container",
                    "args": {
                      "color": "999",
                      "height": 100.0,
                      "child": {
                        "type": "center",
                        "args": {
                          "child": {
                            "type": "column",
                            "args": {
                              "mainAxisSize": "min",
                              "children": [
                                {
                                  "type": "text",
                                  "args": {
                                    "style": {
                                      "fontWeight": "bold"
                                    },
                                    "text": "pre-container"
                                  }
                                },
                                {
                                  "type": "row",
                                  "args": {
                                    "mainAxisSize": "min",
                                    "children": [
                                      {
                                        "type": "text",
                                        "args": {
                                          "text": "("
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "listen": [
                                          "container.minWidth"
                                        ],
                                        "args": {
                                          "text": "${container.minWidth}"
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "args": {
                                          "text": " x "
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "listen": [
                                          "container.minHeight"
                                        ],
                                        "args": {
                                          "text": "${container.minHeight}"
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "args": {
                                          "text": ") <= ("
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "listen": [
                                          "container.maxWidth"
                                        ],
                                        "args": {
                                          "text": "${container.maxWidth}"
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "args": {
                                          "text": " x "
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "listen": [
                                          "container.maxHeight"
                                        ],
                                        "args": {
                                          "text": "${container.maxHeight}"
                                        }
                                      },
                                      {
                                        "type": "text",
                                        "args": {
                                          "text": ")"
                                        }
                                      }
                                    ]
                                  }
                                }
                              ]
                            }
                          }
                        }
                      }
                    }
                  },
                  {
                    "type": "container",
